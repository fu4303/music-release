    "gatsby": "^2.24.3",
    "gatsby-plugin-emotion": "^4.3.10",
    "gatsby-plugin-netlify": "^2.3.11",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gotrue-js": "^0.9.26",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.0.0",
